    "start": "react-scripts start",
    "build": "react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!react-syntax-highlighter)/\"",
    "test:coverage": "yarn test --coverage=true --coverageReporters=cobertura --watchAll=false",
    "test:debug": "react-scripts --inspect-brk test --transformIgnorePatterns \"node_modules/(?!react-syntax-highlighter)/\" --runInBand --no-cache",
    "refresh": "rm -rf node_modules && rm -rf yarn.lock && yarn cache clean && yarn",
    "lint:code": "eslint 'src/**/*.{js,jsx,mjs,ts,tsx,json}'",
    "lint:style": "prettier --check 'src/**/*.{scss,sass,css}'",
    "lint": "yarn lint:code && yarn lint:style",
    "fix:code": " eslint --fix 'src/**/*.{js,jsx,mjs,ts,tsx,json}'",
    "fix:style": "prettier --write 'src/**/*.{scss,sass,css}'",
    "fix": "yarn fix:code && yarn fix:style",
    "eject": "react-scripts eject"
  },
